{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": 1,
    "prettier/prettier": ["error"]
  },
  "plugins": ["import", "jest"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true,
    "jest": true
  }
}
